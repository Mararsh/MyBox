<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="2.0" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.PDFsTableController">
    <children>
        <FlowPane hgap="5.0" maxWidth="1.7976931348623157E308" prefWrapLength="800.0" vgap="5.0" VBox.vgrow="NEVER">
            <children>
                <Button fx:id="addFilesButton" mnemonicParsing="false" onAction="#addFilesAction" onMouseEntered="#popFileAdd">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconAddFile.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="addDirectoryButton" mnemonicParsing="false" onAction="#addDirectoryAction" onMouseEntered="#popDirectoryAdd">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconAddFolder.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="insertFilesButton" mnemonicParsing="false" onAction="#insertFilesAction" onMouseEntered="#popFileInsert">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconInsertFile.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="insertDirectoryButton" mnemonicParsing="false" onAction="#insertDirectoryAction" onMouseEntered="#popDirectoryInsert">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconInsertFolder.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="upFilesButton" mnemonicParsing="false" onAction="#upFilesAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconUp.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="downFilesButton" mnemonicParsing="false" onAction="#downFilesAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconDown.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="viewFileButton" mnemonicParsing="false" onAction="#viewFileAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconView.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="selectAllFilesButton" mnemonicParsing="false" onAction="#selectAllFilesAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconSelectAll.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="unselectAllFilesButton" mnemonicParsing="false" onAction="#unselectAllFilesAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconSelectNone.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="deleteFilesButton" mnemonicParsing="false" onAction="#deleteFilesAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconDeleteFile.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="clearFilesButton" mnemonicParsing="false" onAction="#clearFilesAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconClear.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <CheckBox fx:id="tableSubdirCheck" mnemonicParsing="false" selected="true">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconFolder.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </CheckBox>
                <CheckBox fx:id="tableCreateDirCheck" mnemonicParsing="false" selected="true">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconAddFoloder.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </CheckBox>
                <CheckBox fx:id="tableExpandDirCheck" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconUnfold.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </CheckBox>
                <ToggleButton fx:id="moreButton" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconMore.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </ToggleButton>
            </children>
        </FlowPane>
        <Label fx:id="tableCommentsLabel" layoutX="10.0" layoutY="522.0" styleClass="blue-label" stylesheets="@../styles/MyBox.css" text="%PdfTableComments" wrapText="true" />
        <TableView fx:id="tableView" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="handledColumn" editable="false" prefWidth="180.0" text="%Handled" />
                <TableColumn fx:id="fileColumn" editable="false" maxWidth="1.7976931348623157E308" prefWidth="240.0" text="%File" />
                <TableColumn fx:id="typeColumn" editable="false" minWidth="0.0" prefWidth="100.0" text="%Type" />
                <TableColumn fx:id="userPasswordColumn" prefWidth="150.0" text="%UserPassword" />
                <TableColumn fx:id="ownerPasswordColumn" prefWidth="150.0" text="%OwnerPassword" />
                <TableColumn fx:id="fromColumn" prefWidth="100.0" text="%FromPage" />
                <TableColumn fx:id="toColumn" prefWidth="100.0" text="%ToPage" />
                <TableColumn fx:id="numberColumn" editable="false" prefWidth="120.0" text="%FilesNumber" />
                <TableColumn fx:id="sizeColumn" editable="false" prefWidth="160.0" text="%Size" />
                <TableColumn fx:id="modifyTimeColumn" editable="false" prefWidth="160.0" text="%ModifyTime" />
                <TableColumn fx:id="createTimeColumn" editable="false" prefWidth="160.0" text="%CreateTime" />
            </columns>
        </TableView>
        <FlowPane fx:id="setPDFPane" hgap="5.0" vgap="5.0" VBox.vgrow="NEVER">
            <children>
                <Label text="%Password" />
                <TextField fx:id="passwordInput" prefWidth="120.0" />
                <HBox fx:id="fromToBox" alignment="CENTER_LEFT" spacing="5.0">
                    <children>
                        <Label text="%FromPage" />
                        <TextField fx:id="fromInput" prefWidth="70.0" text="1" />
                        <Label text="%ToPage" />
                        <TextField fx:id="toInput" prefWidth="80.0" />
                    </children>
                </HBox>
                <Button fx:id="pdfSetButton" mnemonicParsing="false" onAction="#setAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconEqual.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Label fx:id="setLabel" styleClass="blue-label" stylesheets="@../styles/MyBox.css" text="%PdfTableInputComments" />
            </children>
        </FlowPane>
        <Label fx:id="tableLabel" styleClass="blue-label" stylesheets="@../styles/MyBox.css" VBox.vgrow="NEVER" />
        <FlowPane fx:id="selectPane" hgap="5.0" layoutX="10.0" layoutY="504.0" vgap="5.0">
            <children>
                <Label text="%FilesSelector" />
                <ComboBox fx:id="nameFiltersSelector" prefHeight="30.0" prefWidth="257.0" />
                <TextField fx:id="tableFiltersInput" prefWidth="400.0" promptText="%SeparateBySpace" />
                <Hyperlink fx:id="regexLink" onAction="#regexHelp" text="%RegularExpression" />
            </children>
        </FlowPane>
    </children>
</VBox>
