<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ImageManufactureViewController">
    <children>
      <FlowPane hgap="5.0" layoutX="10.0" layoutY="176.0" vgap="5.0" VBox.vgrow="NEVER">
         <children>
                <CheckBox fx:id="coordinateCheck" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconLocation.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </CheckBox>
                <CheckBox fx:id="rulerXCheck" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconXRuler.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </CheckBox>
                <CheckBox fx:id="rulerYCheck" mnemonicParsing="false">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/iconYRuler.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </CheckBox>
         </children>
      </FlowPane>
          <Label text="%ZoomStep" />
        <FlowPane hgap="5.0" vgap="5.0" VBox.vgrow="NEVER">
            <children>
                <ComboBox fx:id="zoomStepSelector" prefWidth="120.0" />
                <Label fx:id="zoomValue" text="\%">
                    <padding>
                        <Insets right="30.0" />
                    </padding>
                </Label>
            </children>
        </FlowPane>
      <CheckBox fx:id="contextMenuCheck" mnemonicParsing="false" selected="true" text="%ContextMenu" />
    </children>
</VBox>
