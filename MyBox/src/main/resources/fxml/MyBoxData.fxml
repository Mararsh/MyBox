                                                    </children>
                                                    <padding>
                                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                    </padding>
                                                </VBox>
                                            </content>
                                        </Tab>
                                        <Tab fx:id="colorsTab" text="%TableDefinition">
                                            <content>
                                                <VBox fx:id="colorsBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0">
                                                    <children>
                                                        <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true" prefViewportWidth="721.0" vbarPolicy="ALWAYS" VBox.vgrow="ALWAYS">
