                                                    <image>
                                                        <Image url="@../buttons/Red/iconDelete.png" />
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </Button>
                                        <Button fx:id="addButton" mnemonicParsing="false" onAction="#addAction">
