<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ImageManufactureArcController">
    <children>
        <FlowPane hgap="5.0" prefWidth="200.0" prefWrapLength="0.0" vgap="5.0">
            <children>
                <Label text="%Background" />
                <fx:include fx:id="colorSet" source="ColorSet.fxml" />
            </children>
        </FlowPane>
        <FlowPane hgap="5.0" layoutX="22.0" layoutY="22.0" prefWidth="200.0" prefWrapLength="0.0" vgap="5.0">
            <children>
                <Label text="%Size" />
                <ComboBox fx:id="arcBox" editable="true" prefWidth="100.0" />
            </children>
        </FlowPane>
        <Button fx:id="okButton" mnemonicParsing="false" onAction="#okAction">
            <graphic>
                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../buttons/iconOK.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</VBox>
