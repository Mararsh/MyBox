        <TreeNode>
            <NodeAttributes>
                <title>
                    <![CDATA[Format Date]]>
                </title>
                <match_true>
                    <![CDATA[true]]>
                </match_true>
                <script>
                    <![CDATA[function formatDate(date) {
     var y = date.getFullYear();
     var m = date.getMonth() + 1;
     m = m < 10 ? ('0' + m) : m;
     var d = date.getDate();
     d = d < 10 ? ('0' + d) : d;
     var h =date.getHours();
     h = h < 10 ? ('0' + h) : h;
     var M =date.getMinutes();
     M = M < 10 ? ('0' + M) : M;
     var s =date.getSeconds();
     s = s < 10 ? ('0' + s) : s;
     return y + '-' + m + '-' + d + ' ' + h + ':' + M + ':' + s;
}
if (#{dateColumn} != null)
   formatDate(new Date(#{dateColumn}));
else
   null;]]>
                </script>
            </NodeAttributes>
        </TreeNode>
