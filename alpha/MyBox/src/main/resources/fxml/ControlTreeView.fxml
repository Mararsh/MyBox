<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="2.0" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ControlTreeView">
   <children>
      <TreeTableView fx:id="treeView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
         <columns>
            <TreeTableColumn fx:id="titleColumn" prefWidth="200.0" text="%Title" />
            <TreeTableColumn fx:id="hierarchyNumberColumn" prefWidth="120.0" text="%HierarchyNumber" />
            <TreeTableColumn fx:id="childrenColumn" text="%ChildrenSize" />
            <TreeTableColumn fx:id="idColumn" text="%ID" />
            <TreeTableColumn fx:id="timeColumn" prefWidth="180.0" text="%ModifiedTime" />
            <TreeTableColumn fx:id="orderColumn" text="%OrderNumber" />
         </columns>
      </TreeTableView>
      <Label fx:id="treeLabel" />
   </children>
</VBox>
