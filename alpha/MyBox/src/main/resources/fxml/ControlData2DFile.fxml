<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="2.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ControlData2D">
    <children>
      <HBox fx:id="buttonsBox" alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" spacing="2.0">
         <children>
            <Button fx:id="selectFileButton" mnemonicParsing="false" onAction="#pickSourceFile" onMouseEntered="#popSourceFile">
               <graphic>
                  <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../buttons/Red/iconSelectFile.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="selectButton" mnemonicParsing="false" onAction="#selectAction">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../buttons/Red/iconSelect.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="loadContentInSystemClipboard2Button" mnemonicParsing="false" onAction="#loadContentInSystemClipboard">
               <graphic>
                  <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../buttons/iconImageSystem.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="examplesButton" mnemonicParsing="false" onAction="#showExamplesMenu" onMouseEntered="#popExamplesMenu">
               <graphic>
                  <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../buttons/Red/iconExamples.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="createFileButton" mnemonicParsing="false" onAction="#createAction">
               <graphic>
                  <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../buttons/Red/iconAdd.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <ImageView fx:id="tipsView" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../buttons/Red/iconTips.png" />
               </image>
            </ImageView>
                <FlowPane fx:id="buttonsPane" alignment="CENTER_RIGHT" hgap="2.0" maxWidth="1.7976931348623157E308" prefWrapLength="0.0" vgap="2.0" HBox.hgrow="ALWAYS">
                    <children>
                  <CheckBox fx:id="toolbarCheck" mnemonicParsing="false">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconPanes.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </CheckBox>
                  <Button fx:id="chartsButton" mnemonicParsing="false" onAction="#showChartsMenu" onMouseEntered="#popChartsMenu">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconCharts.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="calculateButton" mnemonicParsing="false" onAction="#showCalculateMenu" onMouseEntered="#popCalculateMenu">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconCalculator.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="trimDataButton" mnemonicParsing="false" onAction="#showTrimMenu" onMouseEntered="#popTrimMenu">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconClean.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="dataButton" mnemonicParsing="false" onAction="#showDataMenu" onMouseEntered="#popDataMenu">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconData.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="operationButton" mnemonicParsing="false" onAction="#showOperationsMenu" onMouseEntered="#popOperationsMenu">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconOperation.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                    </children>
                </FlowPane>
         </children>
      </HBox>
      <VBox fx:id="mainAreaBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="2.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox fx:id="toolbar" alignment="CENTER_LEFT" spacing="2.0">
               <children>
                  <Button fx:id="fileMenuButton" mnemonicParsing="false" onAction="#showFileMenu" onMouseEntered="#popFileMenu">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconFile.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="helpButton" mnemonicParsing="false" onAction="#showHelps" onMouseEntered="#popHelps">
                     <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../Mybox归档/2024-1-17-v6.7.9/MyBox-src/MyBox/src/main/resources/buttons/iconClaw.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <FlowPane alignment="CENTER_RIGHT" hgap="2.0" maxWidth="1.7976931348623157E308" vgap="2.0" HBox.hgrow="ALWAYS">
                     <children>
                        <CheckBox fx:id="lostFocusCommitCheck" mnemonicParsing="false" onAction="#autoCommitCheck">
                           <graphic>
                              <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../../../Mybox归档/2024-1-17-v6.7.9/MyBox-src/MyBox/src/main/resources/buttons/red/iconInput.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </CheckBox>
                        <Button fx:id="verifyButton" mnemonicParsing="false" onAction="#verifyAction">
                           <graphic>
                              <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../../../Mybox归档/2024-1-17-v6.7.9/MyBox-src/MyBox/src/main/resources/buttons/Red/iconVerify.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button fx:id="deleteRowsButton" mnemonicParsing="false" onAction="#deleteRowsAction">
                           <graphic>
                              <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../../../Mybox归档/2024-1-17-v6.7.9/MyBox-src/MyBox/src/main/resources/buttons/Red/iconDelete.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button fx:id="editButton" mnemonicParsing="false" onAction="#editAction">
                           <graphic>
                              <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../../../Mybox归档/2024-1-17-v6.7.9/MyBox-src/MyBox/src/main/resources/buttons/Red/iconEdit.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button fx:id="addRowsButton" mnemonicParsing="false" onAction="#addRowsAction">
                           <graphic>
                              <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../../../Mybox归档/2024-1-17-v6.7.9/MyBox-src/MyBox/src/main/resources/buttons/Red/iconNewItem.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button fx:id="recoverButton" mnemonicParsing="false" onAction="#recoverAction">
                           <graphic>
                              <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../buttons/Red/iconRecover.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveAction">
                           <graphic>
                              <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../buttons/Red/iconSave.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                  </FlowPane>
               </children>
            </HBox>
              <TableView fx:id="tableView" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
                  <columns> 
                      <TableColumn fx:id="rowsSelectionColumn" maxWidth="1.7976931348623157E308" prefWidth="-1.0">
                          <graphic>
                              <CheckBox fx:id="allRowsCheck" mnemonicParsing="false" text="%TableRow" />
                          </graphic>
                      </TableColumn>
                      <TableColumn fx:id="dataRowColumn" editable="false" maxWidth="1.7976931348623157E308" prefWidth="-1.0" text="%DataRow" />
                  </columns>
              </TableView>
            <FlowPane fx:id="paginationPane" alignment="CENTER_RIGHT" hgap="2.0" vgap="2.0">
               <children>
                  <Label fx:id="selectedLabel" minHeight="-Infinity" wrapText="true">
                     <FlowPane.margin>
                        <Insets right="20.0" />
                     </FlowPane.margin>
                  </Label>
                  <Label fx:id="dataSizeLabel" minHeight="-Infinity" wrapText="true">
                     <FlowPane.margin>
                        <Insets right="20.0" />
                     </FlowPane.margin>
                  </Label>
                  <Label text="%PageSize" />
                  <ComboBox fx:id="pageSizeSelector" editable="true" prefWidth="80.0" />
                  <Label text="%Page" />
                  <ComboBox fx:id="pageSelector" editable="true" prefWidth="80.0" />
                  <Label fx:id="pageLabel" />
                  <Button fx:id="goPageButton" mnemonicParsing="false" onAction="#goPage">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconGo.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="pageFirstButton" mnemonicParsing="false" onAction="#pageFirstAction">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconFirst.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="pageLastButton" mnemonicParsing="false" onAction="#pageLastAction">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconLast.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="pagePreviousButton" mnemonicParsing="false" onAction="#pagePreviousAction">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconPrevious.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="pageNextButton" mnemonicParsing="false" onAction="#pageNextAction">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../buttons/Red/iconNext.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </FlowPane>
         </children>
      </VBox>
    </children>
</VBox>
