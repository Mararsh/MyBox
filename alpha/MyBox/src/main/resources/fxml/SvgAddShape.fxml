                                                            </children>
                                                        </FlowPane>
                                                    </children>
                                                </VBox>
                                            </content>
                                        </Tab>
                                        <Tab text="line">
                                            <content>
                                                <VBox fx:id="lineBox" minHeight="-Infinity" spacing="5.0" styleClass="focusBox" stylesheets="@../styles/MyBox.css">
                                                    <children>
                                                        <Label text="%LeftTop" />
                                                        <FlowPane hgap="5.0" minHeight="-Infinity" vgap="5.0">
                                                            <children>
                                                                <Label text="x">
                                                                    <FlowPane.margin>
                                                                        <Insets />
                                                                    </FlowPane.margin>
                                                                </Label>
                                                                <TextField fx:id="rectLeftTopXInput11" prefWidth="100.0" />
                                                            </children>
                                                        </FlowPane>
                                                        <FlowPane hgap="5.0" minHeight="-Infinity" vgap="5.0">
                                                            <children>
                                                                <Label text="y" />
                                                                <TextField fx:id="rectLeftTopYInput11" prefWidth="100.0" />
                                                            </children>
                                                        </FlowPane>
                                                        <Label text="%RightBottom" />
                                                        <FlowPane hgap="5.0" minHeight="-Infinity" vgap="5.0">
                                                            <children>
                                                                <Label text="x">
                                                                    <FlowPane.margin>
                                                                        <Insets />
                                                                    </FlowPane.margin>
                                                                </Label>
                                                                <TextField fx:id="rightBottomXInput11" prefWidth="100.0" />
                                                            </children>
                                                        </FlowPane>
                                                        <FlowPane hgap="5.0" minHeight="-Infinity" vgap="5.0">
                                                            <children>
                                                                <Label text="y" />
                                                                <TextField fx:id="rightBottomYInput11" prefWidth="100.0" />
                                                            </children>
                                                        </FlowPane>
                                                    </children>
                                                </VBox>
                                            </content>
                                        </Tab>
                                        <Tab fx:id="pointsTab1" text="polyline">
                                            <content>
                                                <VBox fx:id="polylineBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" styleClass="focusBox" stylesheets="@../styles/MyBox.css" VBox.vgrow="NEVER" />
                                            </content>
                                        </Tab>
                                        <Tab fx:id="pointsTab" text="polygon">
                                            <content>
                                                <VBox fx:id="polygonBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" styleClass="focusBox" stylesheets="@../styles/MyBox.css" VBox.vgrow="NEVER" />
                                            </content>
                                        </Tab>
                                        <Tab text="path">
                                            <content>
                                                <VBox fx:id="pathBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="2.0">
