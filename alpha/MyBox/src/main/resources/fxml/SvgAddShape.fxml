                                                                <Label text="%Width">
                                                                    <FlowPane.margin>
                                                                        <Insets />
                                                                    </FlowPane.margin>
                                                                </Label>
                                                                <TextField fx:id="rightBottomXInput" prefWidth="100.0" />
