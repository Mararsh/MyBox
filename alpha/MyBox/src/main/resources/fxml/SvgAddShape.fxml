                                    <ScrollPane fx:id="shapePane" fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true" />
                                 </content>
                              </Tab>
                                        <Tab fx:id="styleTab" text="%Style">
