                                                            </children>
                                                        </FlowPane>
                                                        <FlowPane hgap="5.0" prefWidth="200.0" prefWrapLength="0.0" vgap="5.0">
                                                            <children>
                                                                <Label minHeight="-Infinity" text="%StrokeColor" wrapText="true" />
