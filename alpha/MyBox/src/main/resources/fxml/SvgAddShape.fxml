                                                            </children>
                                                        </FlowPane>
                                                        <FlowPane hgap="5.0" layoutX="15.0" layoutY="109.0" prefWidth="200.0" prefWrapLength="0.0" vgap="5.0">
                                                            <children>
                                                                <Label minHeight="-Infinity" text="%StrokeDasharray" wrapText="true" />
                                                                <TextField prefWidth="160.0" />
                                                            </children>
                                                        </FlowPane>
                                                        <HBox alignment="CENTER_LEFT" spacing="2.0">
                                                            <children>
                                                                <Label maxWidth="1.7976931348623157E308" text="style" HBox.hgrow="ALWAYS" />
