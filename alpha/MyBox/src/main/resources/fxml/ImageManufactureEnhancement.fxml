<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ImageManufactureEnhancementController">
    <children>
        <fx:include fx:id="options" source="ImageManufactureEnhancementOptions.fxml" />
        <HBox alignment="CENTER_LEFT" spacing="5.0">
            <children>
                <Button fx:id="okButton" onAction="#okAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconOK.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <CheckBox fx:id="scopeCheck" mnemonicParsing="false">
                    <graphic> 
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconTarget.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </CheckBox>
            </children>
        </HBox>
        <Label fx:id="commentsLabel" styleClass="blue-label" stylesheets="@../styles/MyBox.css" text="%DefineScopeAndManufacture" wrapText="true" VBox.vgrow="NEVER" />
        <Button fx:id="demoButton" mnemonicParsing="false" onAction="#demo">
            <graphic>
                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../buttons/Red/iconDemo.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</VBox>
