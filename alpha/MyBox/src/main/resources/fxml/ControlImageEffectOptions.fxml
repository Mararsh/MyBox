<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ControlImageEffectOptions">
   <children>
      <FlowPane hgap="5.0" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefWidth="80.0" prefWrapLength="200.0" styleClass="focusBox" stylesheets="@../styles/MyBox.css" vgap="5.0">
         <children>
            <RadioButton fx:id="ReduceColorsRadio" layoutX="103.0" layoutY="10.0" selected="true" text="%ReduceColors">
               <toggleGroup>
                  <ToggleGroup fx:id="effectTypeGroup" />
               </toggleGroup>
            </RadioButton>
            <RadioButton fx:id="ThresholdingRadio" layoutX="181.0" layoutY="10.0" text="%Thresholding" toggleGroup="$effectTypeGroup" />
            <RadioButton fx:id="GrayRadio" layoutX="10.0" layoutY="10.0" text="%Gray" toggleGroup="$effectTypeGroup" />
            <RadioButton fx:id="SepiaRadio" layoutX="10.0" layoutY="35.0" text="%Sepia" toggleGroup="$effectTypeGroup" />
            <RadioButton fx:id="BlackOrWhiteRadio" layoutX="88.0" layoutY="35.0" text="%BlackOrWhite" toggleGroup="$effectTypeGroup" />
            <RadioButton fx:id="EdgeDetectionRadio" layoutX="200.0" layoutY="35.0" text="%EdgeDetection" toggleGroup="$effectTypeGroup" />
            <RadioButton fx:id="EmbossRadio" layoutX="10.0" layoutY="85.0" text="%Emboss" toggleGroup="$effectTypeGroup" />
            <RadioButton fx:id="effectMosaicRadio" mnemonicParsing="false" text="%Mosaic" toggleGroup="$effectTypeGroup" />
            <RadioButton fx:id="effectFrostedRadio" mnemonicParsing="false" text="%FrostedGlass" toggleGroup="$effectTypeGroup" />
         </children>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </FlowPane>
      <VBox fx:id="setBox" spacing="5.0">
         <children>
            <VBox fx:id="quanBox" layoutX="10.0" layoutY="538.0" spacing="2.0" styleClass="focusBox" stylesheets="@../styles/MyBox.css">
               <children>
                  <fx:include fx:id="quantization" source="ControlImageQuantization.fxml" />
               </children>
               <padding>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </padding>
            </VBox>
            <FlowPane fx:id="stringBoxPane" hgap="5.0" layoutX="32.0" layoutY="421.0" maxWidth="1.7976931348623157E308" prefWidth="100.0" vgap="5.0">
               <children>
                  <Label fx:id="stringLabel" text="%Value" />
                  <ComboBox fx:id="stringBox" editable="true" maxWidth="1.7976931348623157E308" prefWidth="200.0" />
               </children>
            </FlowPane>
            <FlowPane fx:id="intBoxPane" hgap="5.0" maxWidth="1.7976931348623157E308" prefWidth="100.0" vgap="5.0">
               <children>
                  <Label fx:id="intBoxLabel" text="l" />
                  <ComboBox fx:id="intBox" editable="true" prefWidth="100.0" />
               </children>
            </FlowPane>
            <FlowPane fx:id="intInputPane" hgap="5.0" maxWidth="1.7976931348623157E308" prefWidth="100.0" vgap="5.0">
               <children>
                  <Label fx:id="intLabel" text="l" />
                  <TextField fx:id="intInput" prefWidth="80.0" />
               </children>
            </FlowPane>
            <FlowPane fx:id="intInputPane2" hgap="5.0" maxWidth="1.7976931348623157E308" prefWidth="100.0" vgap="5.0">
               <children>
                  <Label fx:id="intLabel2" text="l" />
                  <TextField fx:id="intInput2" prefWidth="80.0" />
               </children>
            </FlowPane>
            <FlowPane fx:id="intInputPane3" hgap="5.0" maxWidth="1.7976931348623157E308" prefWidth="100.0" vgap="5.0">
               <children>
                  <Label fx:id="intLabel3" text="l" />
                  <TextField fx:id="intInput3" prefWidth="80.0" />
                  <ImageView fx:id="imageThresholdTipsView" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../buttons/Red/iconTips.png" />
                     </image>
                  </ImageView>
               </children>
            </FlowPane>
            <FlowPane fx:id="othersPane" hgap="5.0" maxWidth="1.7976931348623157E308" prefWidth="100.0" vgap="5.0">
               <children>
                  <CheckBox fx:id="valueCheck" mnemonicParsing="false" text="CheckBox" />
               </children>
            </FlowPane>
            <VBox fx:id="binrayBox" styleClass="focusBox" stylesheets="@../styles/MyBox.css">
               <children>
                  <fx:include fx:id="binary" source="ControlImageBinary.fxml" />
               </children>
               <padding>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </padding>
            </VBox>
            <VBox fx:id="edgeBox" layoutX="10.0" layoutY="10.0" spacing="5.0" styleClass="focusBox" stylesheets="@../styles/MyBox.css">
               <children>
                  <Label text="%Algorithm" />
                  <RadioButton fx:id="eightLaplaceRadio" mnemonicParsing="false" selected="true" text="%EightNeighborLaplace">
                     <toggleGroup>
                        <ToggleGroup fx:id="edgeGroup" />
                     </toggleGroup>
                  </RadioButton>
                  <RadioButton fx:id="eightLaplaceExcludedRadio" mnemonicParsing="false" text="%EightNeighborLaplaceInvert" toggleGroup="$edgeGroup" />
                  <RadioButton fx:id="fourLaplaceRadio" mnemonicParsing="false" text="%FourNeighborLaplace" toggleGroup="$edgeGroup" />
                  <RadioButton fx:id="fourLaplaceExcludedRadio" layoutX="15.0" layoutY="97.0" mnemonicParsing="false" text="%FourNeighborLaplaceInvert" toggleGroup="$edgeGroup" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</VBox>
